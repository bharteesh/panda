<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/CatSourceSentToHclList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catSourceSentToHclHome.wire}"/>

   <param name="catSourceSentToHclFrom"/>
   <param name="catSourceSentToHclId" value="#{catSourceSentToHclHome.catSourceSentToHclId}"/>
   <param name="id" value="#{catSourceSentToHclHome.catSourceSentToHclId}"/>


   <navigation from-action="#{catSourceSentToHclHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/sourcesenttohcl/SourceSentToHclEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catSourceSentToHclHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/sourcesenttohcl/SourceSentToHclEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catSourceSentToHclHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatSourceSentToHclList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/sourcesenttohcl/SourceSentToHclEdit.xhtml"/>
            </rule>
     </navigation>


</page>

<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/CatPublishersList.xhtml"
      login-required="false">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catSigsHome.wire}"/>


   <param name="catSigsFrom"/>
   <param name="catSigsId" value="#{catSigsHome.catSigsId}"/>
   <param name="id" value="#{catSigsHome.catSigsId}"/>


   <navigation from-action="#{catSigsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/sigs/SigsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catSigsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/sigs/SigsEdit.xhtml"/>
      </rule>
   </navigation>

   
   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/sigs/SigsEdit.xhtml"/>
            </rule>
     </navigation>

</page>

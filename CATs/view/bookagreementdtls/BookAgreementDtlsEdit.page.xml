<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/CatBookAgreementDtlsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catBookAgreementDtlsHome.wire}"/>
   <param name="catBookAgreementDtlsFrom"/>
   <param name="id" value="#{catBookAgreementDtlsHome.id}"/>
   <param name="catBookAgreementDtlsId" value="#{catBookAgreementDtlsHome.catBookAgreementDtlsId}"/>
   <param name="selectedTab" value="#{catBookAgreementDtlsHome.selectedTab}"/>

   <navigation from-action="#{catBookAgreementDtlsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/bookagreementdtls/BookAgreementDtlsEdit.xhtml"/>
      </rule>
   </navigation>
   <navigation from-action="#{catBookAgreementDtlsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/bookagreementdtls/BookAgreementDtlsEdit.xhtml"/>
      </rule>
   </navigation>
   
   <navigation from-action="#{catBookAgreementDtlsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatBookAgreementDtlsList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
   		<rule if-outcome="cancel" >
         <end-conversation before-redirect="true"/>
         <redirect view-id="/bookagreementdtls/BookAgreementDtlsEdit.xhtml"/>
    	</rule>
   </navigation>


</page>

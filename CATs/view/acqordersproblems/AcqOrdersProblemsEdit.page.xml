<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/CatAcqOrdersProblemsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catAcqOrdersProblemsHome.wire}"/>

   <param name="catAcqOrdersProblemsFrom"/>
   <param name="catAcqOrdersProblemsId" value="#{catAcqOrdersProblemsHome.catAcqOrdersProblemsId}"/>
   <param name="id" value="#{catAcqOrdersProblemsHome.catAcqOrdersProblemsId}"/>
   <param name="catAcqOrdersFrom"/>
   <param name="catAcqOrdersId" value="#{catAcqOrdersHome.catAcqOrdersId}"/>
   <param name="selectedTab" value="#{catAcqOrdersHome.selectedTab}"/>


   <navigation from-action="#{catAcqOrdersProblemsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/acqordersproblems/AcqOrdersProblemsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catAcqOrdersProblemsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/acqordersproblems/AcqOrdersProblemsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catAcqOrdersProblemsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatAcqOrdersProblemsList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
         <redirect view-id="/acqordersproblems/AcqOrdersProblemsEdit.xhtml"/>
    </rule>
     </navigation>


</page>

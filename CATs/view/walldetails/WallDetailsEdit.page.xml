<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd" 
      no-conversation-view-id="/lists/CatWallDtlsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catWallDtlsHome.wire}"/>

   <param name="catWallDtlsFrom"/>
   <param name="catWallDtlsId" value="#{catWallDtlsHome.catWallDtlsId}"/>
   <param name="id" value="#{catWallDtlsHome.catWallDtlsId}"/>
   <param name="notesSortOrder" value="#{catWallDtlsHome.notesSortOrder}"/>
   <param name="selectedTab" value="#{catWallDtlsHome.selectedTab}"/>

   <param name="catSetsFrom"/>
   <param name="catSetsId" value="#{catSetsHome.catSetsId}"/>


   <navigation from-action="#{catWallDtlsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/walldetails/WallDetailsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catWallDtlsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/walldetails/WallDetailsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catWallDtlsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatWallDtlsList.xhtml"/>
      </rule>
   </navigation>

</page>

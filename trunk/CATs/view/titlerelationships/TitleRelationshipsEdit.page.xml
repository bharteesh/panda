<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/CatTitleRltnshpsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catTitleRltnshpsHome.wire}"/>

   <param name="catTitleRltnshps"/>
   <param name="catTitleRltnshpsId" value="#{catTitleRltnshpsHome.id}"/>
   <param name="id" value="#{catTitleRltnshpsHome.id}"/>
   

   <navigation from-action="#{catTitleRltnshpsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/titlerelationships/TitleRelationshipsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catTitleRltnshpsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/titlerelationships/TitleRelationshipsEdit.xhtml"/>
      </rule>
   </navigation>
   
   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/home.xhtml"/>
            </rule>
   </navigation>
    <navigation from-action="#{catTitleRltnshpsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatTitleRltnshpsList.xhtml"/>
      </rule>
   </navigation>

</page>

<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/CatWallOverridesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catWallOverridesHome.wire}"/>

   <param name="catWallOverridesFrom"/>
   <param name="catWallOverridesId" value="#{catWallOverridesHome.catWallOverridesId}"/>
   <param name="id" value="#{catWallOverridesHome.catWallOverridesId}"/>
   <param name="catSetsFrom"/>
   <param name="catSetsId" value="#{catSetsHome.catSetsId}"/>
   <param name="selectedTab" value="#{catWallOverridesHome.selectedTab}"/>


   <navigation from-action="#{catWallOverridesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/walloverrides/WallOverridesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catWallOverridesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/walloverrides/WallOverridesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catWallOverridesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatWallOverridesList.xhtml"/>
      </rule>
   </navigation>

</page>

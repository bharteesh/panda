<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/CatPublishersToCmptsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catPublishersToCmptsHome.wire}"/>

   <param name="catPublishersToCmptsFrom"/>
   <param name="catPublishersToCmptsId" value="#{catPublishersToCmptsHome.id}"/>
   <param name="id" value="#{catPublishersToCmptsHome.id}"/>
  <param name="selectedTab" value="#{catPublishersToCmptsHome.selectedTab}"/>

   <navigation from-action="#{catPublishersToCmptsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/publisherRoles/PublisherRolesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catPublishersToCmptsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/publisherRoles/PublisherRolesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catPublishersToCmptsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatPublishersToCmptsList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/publisherRoles/PublisherRolesEdit.xhtml"/>
            </rule>
     </navigation>


</page>

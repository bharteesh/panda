<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/CatLtmDistributionsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catLtmDistributionsHome.wire}"/>
   <param name="catLtmDistributionsFrom"/>
   <param name="id" value="#{catLtmDistributionsHome.id}"/>
   <param name="catLtmDistributionsId" value="#{catLtmDistributionsHome.catLtmDistributionsId}"/>
   <param name="selectedTab" value="#{catLtmDistributionsHome.selectedTab}"/>

   <navigation from-action="#{catLtmDistributionsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/LtmDistributionsEdit.xhtml"/>
      </rule>
   </navigation>
   <navigation from-action="#{catLtmDistributionsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/LtmDistributionsEdit.xhtml"/>
      </rule>
   </navigation>
   
   <navigation from-action="#{catLtmDistributionsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/LtmDistributionsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation>
   		<rule if-outcome="cancel" >
         <end-conversation before-redirect="true"/>
         <redirect view-id="/LtmDistributionsEdit.xhtml"/>
    	</rule>
   </navigation>


</page>

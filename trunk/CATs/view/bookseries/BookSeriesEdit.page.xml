<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/CatBookSeriesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catBookSeriesHome.wire}"/>
   <param name="catBookSeriesFrom"/>
   <param name="id" value="#{catBookSeriesHome.catBookSeriesId}"/>
   <param name="catBookSeriesId" value="#{catBookSeriesHome.catBookSeriesId}"/>
   <param name="selectedTab" value="#{catBookSeriesHome.selectedTab}"/>
   <param name="notesSortOrder" value="#{catBookSeriesHome.notesSortOrder}"/>

   <navigation from-action="#{catBookSeriesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/bookseries/BookSeriesEdit.xhtml"/>
      </rule>
   </navigation>
   <navigation from-action="#{catBookSeriesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/bookseries/BookSeriesEdit.xhtml"/>
      </rule>
   </navigation>
   
   <navigation from-action="#{catBookSeriesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatBookSeriesList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
   		<rule if-outcome="cancel" >
         <end-conversation before-redirect="true"/>
         <redirect view-id="/bookseries/BookSeriesEdit.xhtml"/>
    	</rule>
   </navigation>


</page>

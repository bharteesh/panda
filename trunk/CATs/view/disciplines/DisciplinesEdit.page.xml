<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/CatDisciplinesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catDisciplinesHome.wire}"/>

   <param name="catDisciplinesFrom"/>
   <param name="catDisciplinesId" value="#{catDisciplinesHome.id}"/>
   <param name="id" value="#{catDisciplinesHome.id}"/>
	<param name="selectedTab" value="#{catDisciplinesHome.selectedTab}"/>

   <navigation from-action="#{catDisciplinesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/disciplines/DisciplinesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catDisciplinesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/disciplines/DisciplinesEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catDisciplinesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/CatDisciplinesList.xhtml"/>
      </rule>
   </navigation>

   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/disciplines/DisciplinesEdit.xhtml"/>
            </rule>
     </navigation>


</page>

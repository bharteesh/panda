<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/lists/SetsRollupsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{catSetsHome.wire}"/>

   <param name="catSetsFrom"/>
   <param name="catSetsId" value="#{catSetsHome.catSetsId}"/>
   <param name="id" value="#{catSetsHome.catSetsId}"/>
   <param name="notesSortOrder" value="#{catSetsHome.notesSortOrder}"/>
   <param name="selectedTab" value="#{catSetsHome.selectedTab}"/>
   
   <navigation from-action="#{changePublicationStatusWizard.reset}">
            <rule if-outcome="reset">
                <end-conversation before-redirect="true"/>
                <redirect view-id="/sets/SetsEdit.xhtml"/>
            </rule>
     </navigation>

   <navigation from-action="#{catSetsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/sets/SetsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catSetsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/sets/SetsEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{catSetsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/lists/SetsRollupsList.xhtml"/>
      </rule>
   </navigation>
   
   <navigation>
            <rule if-outcome="cancel" >
                <end-conversation before-redirect="true"/>
                <redirect view-id="/sets/SetsEdit.xhtml"/>
            </rule>
     </navigation>

</page>

<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true"
 no-conversation-view-id="/lists/CatPublishersList.xhtml"
 xmlns="http://jboss.com/products/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
 <begin-conversation flush-mode="MANUAL" join="true"/>
 <action execute="#{catPublishersHome.wire}"/>
 <param name="catPublishersFrom"/>
 <param name="id" value="#{catPublishersHome.catPublishersId}"/>
 <param name="catPublishersId" value="#{catPublishersHome.catPublishersId}"/> 
 <param name="notesSortOrder" value="#{catPublishersHome.notesSortOrder}"/>
 <param name="selectedTab" value="#{catPublishersHome.selectedTab}"/>
 <navigation from-action="#{catPublishersHome.persist}">
  <rule if-outcome="persisted">
   <end-conversation/>
   <redirect view-id="/publishers/PublishersEdit.xhtml"/>
  </rule>
 </navigation>
 <navigation from-action="#{catPublishersHome.update}">
  <rule if-outcome="updated">
   <end-conversation/>
   <redirect view-id="/publishers/PublishersEdit.xhtml"/>
  </rule>
 </navigation>
 <navigation from-action="#{catPublishersHome.remove}">
  <rule if-outcome="removed">
   <end-conversation/>
   <redirect view-id="/lists/CatPublishersList.xhtml"/>
  </rule>
 </navigation>
 <navigation>
  <rule if-outcome="cancel">
   <end-conversation before-redirect="true"/>
   <redirect view-id="/publishers/PublishersEdit.xhtml"/>
  </rule>
 </navigation>
</page>
